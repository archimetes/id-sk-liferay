<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 7.0.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_7_0_0.dtd">
<look-and-feel>
	<compatibility>
		<version>7.4.0+</version>
	</compatibility>
	<theme id="lrsk3" name="lrsk3">
		<template-extension>ftl</template-extension>
		<settings>
			<setting configurable="true" key="show-header-search" type="checkbox" value="true"/>
			<setting configurable="false" key="show-site-name-default" type="checkbox" value="true"/>
			<setting configurable="false" key="show-site-name-supported" type="checkbox" value="true"/>
			<setting configurable="true" key="cookie_banner" type="checkbox" value="false"/>
			<setting configurable="true" key="local_navigation" type="checkbox" value="false"/>
			<setting configurable="true" key="show-header" type="checkbox" value="false"/>
			<setting configurable="true" key="show-footer" type="checkbox" value="false"/>
			<setting configurable="true" key="show-breadcrumb" type="checkbox" value="false"/>
			<setting configurable="true" key="show-header-personal-bar" type="checkbox" value="true"/>
			<setting configurable="true" key="inline-navigation" type="checkbox" value="false"/>
			<setting configurable="true" key="show-feedback" type="checkbox" value="false"/>
		</settings>
		<roles>
			<role-name>User</role-name>
		</roles>
		<portlet-decorator id="barebone" name="Barebone">
			<default-portlet-decorator>true</default-portlet-decorator>
			<portlet-decorator-css-class>portlet-barebone</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="borderless" name="Borderless">
			<portlet-decorator-css-class>portlet-borderless</portlet-decorator-css-class>
		</portlet-decorator>
		<portlet-decorator id="decorate" name="Decorate">
			<portlet-decorator-css-class>portlet-decorate</portlet-decorator-css-class>
		</portlet-decorator>
	</theme>
</look-and-feel>
